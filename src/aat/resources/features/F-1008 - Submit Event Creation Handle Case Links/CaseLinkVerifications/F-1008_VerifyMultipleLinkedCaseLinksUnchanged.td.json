{
	"title": "should retrieve case links for a specified case reference",
	"_guid_": "F-1008_VerifyMultipleLinkedCaseLinksUnchanged",
  "_extends_": "F-1008_VerifyCaseLinksBase",

  "specs": [
    "to verify that the Case Links in the CASE_LINK table are unchanged"
  ],

	"request": {
		"pathVariables": {
			"cid": "${[scenarioContext][parentContext][childContexts][F-1008_CreateMultipleLinkedCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}"
		}
	},

	"expectedResponse": {
		"body": {
      "CaseLinks" : [ {
        "CaseId" : "${[scenarioContext][parentContext][childContexts][F-1008_CreateMultipleLinkedCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "LinkedCaseId" : "${[scenarioContext][parentContext][childContexts][F-1008_CreateCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "CaseTypeId" : "FT_MasterCaseType"
      },
      {
        "CaseId" : "${[scenarioContext][parentContext][childContexts][F-1008_CreateMultipleLinkedCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "LinkedCaseId" : "${[scenarioContext][parentContext][childContexts][F-1008_CreateAnotherCasePreRequisiteCaseworkerBase][testData][actualResponse][body][id]}",
        "CaseTypeId" : "FT_MasterCaseType"
      }]
		}
	}
}
